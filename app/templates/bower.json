{
  "name": "capital-framework",
  "version": "0.1.0",
  "description": "Master repository for building the complete Capital Framework.",
  "keywords": [
    "capital-framework",
    "capital",
    "framework",
    "grunt",
    "build"
  ],
  "authors": [
    {
      "name": "Consumer Financial Protection Bureau",
      "url": "http://cfpb.github.io/"
    }
  ],
  "homepage": "https://github.com/cfpb/capital-framework",
  "repository": {
    "type": "git",
    "url": "https://github.com/cfpb/capital-framework.git"
  },
  "license": "https://github.com/cfpb/capital-framework/blob/master/TERMS.md",
  "dependencies": {
    "cf-buttons": "git://github.com/cfpb/cf-buttons.git",
    "cf-core": "git://github.com/cfpb/cf-core.git",
    "cf-expandables": "git://github.com/cfpb/cf-expandables.git",
    "cf-forms": "git://github.com/cfpb/cf-forms.git",
    "cf-grid": "git://github.com/cfpb/cf-grid.git",
    "cf-icons": "git://github.com/cfpb/cf-icons.git",
    "html5shiv": "latest",
    "jquery": "~1.11.0",
    "jquery.easing": "~1.3.0"
  },
  "exportsOverride": {
    "cf-*": {
      "css": "src/**/*.css",
      "less": "src/**/*.less",
      "fonts": "src/fonts/*.*",
      "js": "src/**/*.js"
    },
    "html5shiv": {
      "js": "dist/html5shiv-printshiv.min.js"
    },
    "jquery.easing": {
      "js": "js/jquery.easing.js"
    }
  }
}
